{
    "name": "RawNet2_no_abs_hz_nonzero_sinc_grad",
    "n_gpu": 1,
    "arch": {
      "type": "RawNet2",
      "args": {
        "sinc_type": "s1",
        "norm_before_gru": true,
        "gru_hidden": 1024,
        "gru_num_layers": 3,
        "sinc_out_channels": 20,
        "sinc_filter_length": 1024,
        "sinc_abs": false,
        "sinc_requires_grad": true,
        "min_low_hz": 50,
        "min_band_hz": 50,
        "small_resblock_kernel_size": 20,
        "large_resblock_kernel_size": 128
      }
    },
    "optimizer": {
      "type": "AdamW",
      "args": {
        "lr": 0.0001,
        "weight_decay": 0.0001
      }
    },
    "loss": {
      "type": "CrossEntropyLoss",
      "args": {
        "weight": [1.0, 9.0]
      }
    },
    "lr_scheduler": {
      "type": "ExponentialLR",
      "args": {
        "gamma": 1.0
      }
    },
    "trainer": {
      "epochs": 100,
      "len_epoch": 1000,
      "log_step": 100,
      "grad_norm_clip": 10,
      "monitor": "min dev_loss",
      "early_stop": 2000,
      "save_dir": "saved/",
      "save_period": 5,
      "log_parameters": false,
      "verbosity": 2,
      "visualize": "wandb",
      "wandb_entity": "yaraksen",
      "wandb_project": "anti_spoofing_proj"
    },
    "data": {
      "train": {
        "batch_size": 32,
        "num_workers": 4,
        "datasets": [
          {
            "type": "ASVSpoofDataset",
            "args": {
              "data_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_train",
              "protocol_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.train.trn.txt"
            }
          }
        ]
      },
      "eval": {
        "batch_size": 32,
        "num_workers": 4,
        "datasets": [
          {
            "type": "ASVSpoofDataset",
            "args": {
              "data_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_eval",
              "protocol_path": "/kaggle/input/asvpoof-2019-dataset/LA/LA/ASVspoof2019_LA_cm_protocols/ASVspoof2019.LA.cm.eval.trl.txt"
            }
          }
        ]
      }
    },
    "metrics": [
      {
        "type": "EERMetric",
        "args": {
          "name": "EER"
        }
      }
    ]
  }
  